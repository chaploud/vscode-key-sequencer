{
  "name": "key-sequencer",
  "displayName": "Key Sequencer",
  "description": "Execute commands with your own mnemonic key sequences, inspired by which-key.",
  "publisher": "chaploud",
  "author": {
    "name": "chaploud",
    "url": "https://github.com/chaploud"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Keymaps",
    "Other"
  ],
  "keywords": [
    "which-key",
    "keymap",
    "key-sequencer",
    "keybindings",
    "mnemonic"
  ],
  "license": "MIT",
  "homepage": "https://github.com/chaploud/vscode-key-sequencer",
  "repository": {
    "type": "git",
    "url": "https://github.com/chaploud/vscode-key-sequencer.git"
  },
  "bugs": {
    "url": "https://github.com/chaploud/vscode-key-sequencer/issues"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "key-sequencer.start",
        "title": "Start key sequence listening"
      },
      {
        "command": "key-sequencer.cancel",
        "title": "Cancel key sequence listening"
      }
    ],
    "keybindings": [
      {
        "key": "alt+oem_comma",
        "command": "key-sequencer.start",
        "when": "editorTextFocus"
      },
      {
        "key": "escape",
        "command": "key-sequencer.cancel",
        "when": "key-sequencer.active"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}